local UserInputService = game:GetService("UserInputService")
local SmoothDragSpeed = 0.55

local SmoothDragToggle
local SmoothDragInput
local SmoothDragStart

function SmoothDrag(Frame)
    local function UpdateSmoothDragInput(Input)
        local Delta = Input.Position - SmoothDragStart
        local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
        game:GetService("TweenService"):Create(Frame, TweenInfo.new(SmoothDragSpeed), {Position = Position}):Play()
    end

    Frame.InputBegan:Connect(function(Input)
        if (Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch) and UserInputService:GetFocusedTextBox() == nil then
            SmoothDragToggle = true
            SmoothDragStart = Input.Position
            startPos = Frame.Position
            Input.Changed:Connect(function()
                if Input.UserInputState == Enum.UserInputState.End then
                    SmoothDragToggle = false
                end
            end)
        end
    end)

    Frame.InputChanged:Connect(function(Input)
        if Input.UserInputType == Enum.UserInputType.MouseMovement or Input.UserInputType == Enum.UserInputType.Touch then
            SmoothDragInput = Input
        end
    end)

    game:GetService("UserInputService").InputChanged:Connect(function(Input)
        if Input == SmoothDragInput and SmoothDragToggle then
            UpdateSmoothDragInput(Input)
        end
    end)
end

SmoothDrag(main)

p = Instance.new("Part", workspace)
p.Name = "chlen"
p.Anchored = true
p.Position = Vector3.new(20.598217010498047, 210.19073486328125, 77.20390319824219)
p.Size = Vector3.new(540, 2, 600)
p.Transparency = 1
p.CanCollide = false
p.CanTouch = false

e = true
ae = false
rag = false
dashh = true
dashfr = false

function antifall_f()
	if e == true then
		p.CanCollide = true
		p.CanTouch = true
		e = false
		notify(":D https://t.me/baicovsky_mafia69", "antifall enabled", 5)
		antifall.BackgroundColor3 = Color3.fromRGB(172, 0, 0)
	else
		p.CanCollide = false
		p.CanTouch = false
		e = true
		notify("!!! https://t.me/baicovsky_mafia69", "antifall disabled", 5)
		antifall.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
	end
end

function dash()
    pcall(function()
		if dashh == true then
        	plr.Character.HumanoidRootPart.CFrame = plr.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -10)
		end
    end)
end

function updash()
    pcall(function()
        plr.Character.HumanoidRootPart.CFrame = plr.Character.HumanoidRootPart.CFrame * CFrame.new(0, 10, 0)
    end)
end

function undash()
    pcall(function()
		if dashh == true then
        	plr.Character.HumanoidRootPart.CFrame = plr.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 10)
		end
    end)
end

local rgdl = game.ReplicatedStorage:WaitForChild("Events"):WaitForChild("Ragdoll")
local plr = game.Players.LocalPlayer

function antirag()
	while task.wait(0.000005) do
		if plr.Character:FindFirstChild("Knocked") and rag == true and plr.Character.Humanoid.Health > 0 then
			knock = plr.Character:FindFirstChild("Knocked")
			nocombat = plr.Character:FindFirstChild("NoCombat")
			rgdl:FireServer(false)
			nocombat:Destroy()
			knock:Destroy()
		end
	end
end

function chillzona()
	plr.Character.HumanoidRootPart.CFrame = CFrame.new(11.384864807128906, 370.0810241699219, -33.262428283691406)
end

function chillzona2()
	if ae == false then
		workspace.Map.Timer:GetChildren()[3].Size = Vector3.new(20, 1.0415092706680298, 100)
		plr.Character.HumanoidRootPart.CFrame = CFrame.new(159.6253204345703, 227.04151916503906, -51.46882247924805)
		ae = true
	else
		workspace.Map.Timer:GetChildren()[3].Size = Vector3.new(4.166037559509277, 1.0415092706680298, 46.607540130615234)
		ae = false
	end
end

local function us(input)
	if input.KeyCode == Enum.KeyCode.Q and dashh == true then
		dash()
	elseif input.KeyCode == Enum.KeyCode.R and dashh == true then
		undash()
	elseif input.KeyCode == Enum.KeyCode.Backquote then
		chillzona()
	elseif input.KeyCode == Enum.KeyCode.F1 then
		chillzona2()
	elseif input.KeyCode == Enum.KeyCode.RightShift then
		antifall_f()
	end
end

