local TextChannels = game:GetService("TextChatService"):WaitForChild("TextChannels")
tr = false

conf = {
    lang = "ru" -- change it to your lang
}

local function send(plr, msg) -- send chat msg
    TextChannels.RBXSystem:DisplaySystemMessage("[".. plr .. "]: ".. msg)
end

local function translate(text) -- google translate api
    if text:sub(1, 1) == "#" then -- just dont translate censored msgs
        return text
    end
  local url = "https://translate.googleapis.com/translate_a/single?" ..
        "client=gtx&" ..
        "sl=auto&" ..
        "tl=" .. conf.lang .. "&" ..
        "ie=UTF-8&" .. 
        "oe=UTF-8&" ..  
        "dt=t&" ..      
        "dt=at&" ..    
        "dt=bd&" ..     
        "dt=rm&" ..     
        "dt=ss&" ..     
        "dt=ex&" ..     
            "q=" .. text
    local response = game:HttpGet(url)
    
    local translated = string.match(response, '%[%[%["(.-)"')
    return translated
end

chattran.MouseButton1Click:Connect(function()
	if tr == false then
		tr = true
		chattran.BackgroundColor3 = Color3.fromRGB(172, 0, 0)
	else
		tr = false
		chattran.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
	end
end)

iy.MouseButton1Click:Connect(function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

uis.InputBegan:Connect(us)
game:GetService("TextChatService").MessageReceived:Connect(function(m)
    if m.TextSource and tr == true then
        local p = game:GetService("Players"):GetPlayerByUserId(m.TextSource.UserId)
        if p then print(p.Name .. ": " .. m.Text) end
        send(p.Name, translate(m.Text))
    end
end)
spawn(antirag())
